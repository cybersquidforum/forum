{
    "name": "cybersquids/forum",
    "description": "Laravel forum created with vuejs and Vuetify.",
    "keywords": [
        "cybersquids",
        "laravel",
        "forum"
    ],
    "homepage": "https://github.com/cybersquids/forum",
    "license": "MIT",
    "require": {
        "php" : ">=7.1.0",
        "laravel/framework": "~5.6.0|~5.7.0",
        "predis/predis": ">=1.1.0"
    },
    "autoload": {
        "psr-4": {
            "Cybersquid\\Forum\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cybersquid\\Forum\\ForumServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
